<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#a2bece">

    <TextView
        android:id="@+id/textView16"
        android:layout_width="match_parent"
        android:layout_height="38dp"
        android:layout_marginTop="16dp"
        android:text="Analytics"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:layout_width="411dp"
        android:layout_height="615dp"
        android:layout_marginTop="44dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16">

        <Button
            android:id="@+id/Average_occupancy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Average Daily Occupancy per Parking Space" />

        <Button
            android:id="@+id/monthly_revenue"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Monthly Revenue per Customer" />

        <Button
            android:id="@+id/average_duration"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Average Duration Parked per Customer" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>